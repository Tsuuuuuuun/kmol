{
  "model": {
    "type": "protein_ligand",
    "protein_module": {
      "type": "linear",
      "in_features": 9723,
      "dropout": 0.1,
      "hidden_features": 256,
      "out_features": 128
    },
    "ligand_module": {
      "type": "graph_convolutional",
      "in_features": 45,
      "out_features": 128,
      "hidden_features": 1024,
      "edge_features": 12,
      "dropout": 0.1,
      "layer_type": "torch_geometric.nn.LEConv",
      "layers_count": 3,
      "molecule_features": 17,
      "is_residual": 1,
      "read_out": "attention",
      "norm_layer": "kmol.model.layers.BatchNorm"
    },
    "use_mi": true,
    "xavier_init": false,
    "hidden_features": 64,
    "out_features": 3
  },
  "loader": {
    "type": "csv",
    "input_path": "data/datasets/chembl_superlight.csv",
    "input_column_names": ["smiles", "target_sequence"],
    "target_column_names": ["t_100n", "t_1u", "t_10u"]
    },
  "splitter": {
    "type": "index",
    "splits": {
      "train": 0.8,
      "test": 0.1,
      "validation": 0.1
    }
  },
  "featurizers": [
    {
      "type": "graph",
      "inputs": [
        "smiles"
      ],
      "outputs": [
        "ligand"
      ],
      "descriptor_calculator": {
        "type": "rdkit"
      }
    },
    {
      "type": "bag_of_words",
      "inputs": [
        "target_sequence"
      ],
      "outputs": [
        "protein"
      ],
      "should_cache": true,
      "vocabulary": [
        "A",
        "C",
        "D",
        "E",
        "F",
        "G",
        "H",
        "I",
        "K",
        "L",
        "M",
        "N",
        "P",
        "Q",
        "R",
        "S",
        "T",
        "V",
        "W",
        "Y",
        "X"
      ],
      "max_length": 3,
      "rewrite": false
    }
  ],
  "transformers": [],
  "criterion": {
    "type": "torch.nn.BCEWithLogitsLoss"
  },
  "optimizer": {
    "type": "torch.optim.AdamW",
    "lr": 3e-05,
    "weight_decay": 4e-06
  },
  "scheduler": {
    "type": "torch.optim.lr_scheduler.OneCycleLR",
    "max_lr": 0.00014,
    "epochs": 10,
    "final_div_factor": 1000
  },
  "output_path": "data/logs/kyodai_test",
  "is_stepwise_scheduler": true,
  "is_finetuning": false,
  "checkpoint_path": null,
  "threshold": 0.5,
  "inference_mode": "null",
  "cross_validation_folds": 5,
  "mc_dropout_iterations": 10,
  "mc_dropout_probability": null,
  "probe_layer": null,
  "train_split": "train",
  "train_metrics": [
    "roc_auc",
    "accuracy",
    "pr_auc"
  ],
  "validation_split": "validation",
  "test_split": "test",
  "test_metrics": [
    "roc_auc",
    "accuracy",
    "pr_auc"
  ],
  "epochs": 10,
  "batch_size": 512,
  "drop_last_batch": false,
  "use_cuda": true,
  "enabled_gpus": [
    1
  ],
  "num_workers": 0,
  "featurization_jobs": 4,
  "cache_location": "data/cache/",
  "clear_cache": false,

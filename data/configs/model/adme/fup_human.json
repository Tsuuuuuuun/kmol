{
    "model": {
        "type": "graph_convolutional",
        "in_features": 45,
        "out_features": 1,
        "hidden_features": 128,
        "dropout": 0.15,
        "layer_type": "kmol.model.layers.GINConvolution",
        "layers_count": 2,
        "molecule_features": 17,
        "is_residual": true,
        "norm_layer": "kmol.model.layers.BatchNorm"
    },
    "loader": {
        "type": "csv",
        "input_path": "data/input/adme/ready/fup_human.csv",
        "input_column_names": ["smiles"],
        "target_column_names": ["value"]
    },
    "splitter": {
        "type": "random",
        "splits": {"train": 0.8, "test": 0.2},
        "seed": 42
    },
    "featurizers": [
        {
            "type": "graph",
            "inputs": ["smiles"],
            "outputs": ["graph"],
            "descriptor_calculator": {"type": "rdkit"}
        }
    ],
    "transformers": [],
    "optimizer": {
        "type": "torch.optim.AdamW",
        "lr": 3e-3,
        "weight_decay": 1.5e-6
    },
    "criterion": {
        "type": "torch.nn.SmoothL1Loss"
    },
    "scheduler": {
        "type": "torch.optim.lr_scheduler.OneCycleLR",
        "max_lr": 3e-3,
        "epochs": 200,
        "pct_start": 0.3,
        "div_factor": 25,
        "final_div_factor": 1000
    },

    "output_path": "data/logs/server/",
    "checkpoint_path": null,
    "cross_validation_folds": 5,

    "train_metrics": [],
    "test_metrics": ["r2", "mae", "rmse"],

    "epochs": 200,
    "batch_size": 68,

    "use_cuda": true,
    "enabled_gpus": [0],

    "cache_location": "data/cache/",
    "clear_cache": false,

    "log_level": "info",
    "log_format": "",
    "log_frequency": 5,

    "observers": {
        "before_criterion": ["kmol.core.observers.AddSigmoidHandler"],
        "after_predict": ["kmol.core.observers.AddSigmoidHandler"]
    },
    "target_metric": "r2",
    "optuna_trials": 100
}
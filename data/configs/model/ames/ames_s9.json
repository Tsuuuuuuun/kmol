{
    "model": {
        "type": "graph_convolutional",
        "in_features": 45,
        "out_features": 1,
        "hidden_features": 160,
        "dropout": 0.1,
        "layer_type": "torch_geometric.nn.GCNConv",
        "layers_count": 3,
        "molecule_features": 17,
        "is_residual": 1,
        "norm_layer": "kmol.model.layers.BatchNorm"
    },
    "loader": {
        "type": "excel",
        "input_path": "data/input/ames/ames.xlsx",
        "sheet_index": 2,
        "input_column_names": ["Canonical_SMILES"],
        "target_column_names": ["Label"]
    },
    "splitter": {
        "type": "stratified",
        "seed": 42,
        "target_name": "Label",
        "splits": {"train": 0.8, "test": 0.2}
    },
    "featurizers": [
        {
            "type": "graph",
            "inputs": ["Canonical_SMILES"],
            "outputs": ["graph"],
            "descriptor_calculator": {"type": "rdkit"}
        }
    ],
    "transformers": [],
    "optimizer": {
        "type": "kmol.model.optimizers.AdaBelief",
        "lr": 0.01,
        "weight_decay": 0.00036
    },
    "criterion": {
        "type": "torch.nn.BCEWithLogitsLoss"
    },
    "scheduler": {
        "type": "torch.optim.lr_scheduler.OneCycleLR",
        "max_lr": 0.01,
        "epochs": 200,
        "pct_start": 0.3,
        "div_factor": 25,
        "final_div_factor": 1000
    },
    "output_path": "data/logs/server/",
    "checkpoint_path": null,
    "threshold": 0.4989107847213745,
    "cross_validation_folds": 5,
    "train_metrics": [],
    "test_metrics": ["roc_auc", "pr_auc", "accuracy", "precision", "recall", "cohen_kappa"],
    "epochs": 200,
    "batch_size": 128,
    "use_cuda": true,
    "enabled_gpus": [0],
    "cache_location": "data/cache/",
    "clear_cache": false,
    "log_level": "info",
    "log_format": "",
    "log_frequency": 5,
    "observers": {},
    "target_metric": "roc_auc",
    "optuna_trials": 1000
}
